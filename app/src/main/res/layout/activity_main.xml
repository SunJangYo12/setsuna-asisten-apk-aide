<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:gravity="top|center"
	android:keepScreenOn="true"
	android:layout_gravity="center|fill_vertical|fill_horizontal">

	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="430dp">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="50dp"
					android:id="@+id/main_btn_apkextrak"
					android:text="Apk Extraktor"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_objviewer"
					android:text="Objviewer"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="50dp"
					android:id="@+id/main_btn_remoteclip"
					android:text="Remote Clip"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_readjson"
					android:layout_marginRight="50dp"
					android:text="read json"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="50dp"
					android:id="@+id/main_btn_keyboard"
					android:text="Keyboard"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_mouse"
					android:layout_marginRight="50dp"
					android:text="Mouse"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="50dp"
					android:id="@+id/main_btn_pyautogui"
					android:text="pyautogui"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_remote"
					android:layout_marginRight="50dp"
					android:text="remote audac"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="50dp"
					android:id="@+id/main_btn_browser"
					android:text="Browser"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_filemanager"
					android:text="Filemanager"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="20dp"
					android:id="@+id/main_btn_terminal"
					android:text="Terminal shell"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_task"
					android:text="Taskmanager"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="20dp"
					android:id="@+id/main_btn_server"
					android:text="php server"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_sqlite"
					android:text="sqlite manager"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="30dp"
					android:id="@+id/main_btn_subdomner"
					android:text="subdomner"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_pengaturan"
					android:text="pengaturan"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="30dp"
					android:id="@+id/main_btn_koogps"
					android:text="koordinat gps"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:id="@+id/main_btn_vidtrack"
					android:text="video tracking"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="30dp"
					android:id="@+id/main_btn_exit"
					android:text="Exit"/>

				<Button
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="20dp"
					android:layout_marginRight="30dp"
					android:id="@+id/main_btn_exitfull"
					android:text="Exit full"/>

			</LinearLayout>

		</LinearLayout>

	</ScrollView>

	<LinearLayout
		android:orientation="vertical"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_marginLeft="3dp"
		android:layout_marginRight="3dp"
		android:layout_marginTop="3dp"
		android:background="#FFEE00"
		android:layout_marginBottom="3dp"
		android:gravity="center"
		android:layout_gravity="center|fill_vertical|fill_horizontal">

		<LinearLayout
			android:orientation="vertical"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_marginLeft="5dp"
			android:layout_marginRight="5dp"
			android:layout_marginTop="5dp"
			android:layout_marginBottom="5dp"
			android:background="#000000"
			android:gravity="center"
			android:layout_gravity="center|fill_vertical|fill_horizontal">

			<ScrollView
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="#000000"
				android:layout_marginLeft="5dp"
				android:layout_marginRight="5dp"
				android:layout_marginBottom="5dp"
				android:layout_marginTop="5dp"
				android:id="@+id/console_scroll">

				<TextView
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:textStyle="bold"
					android:textColor="#FFEE00"
					android:typeface="monospace"
					android:id="@+id/console"
					android:textSize="11sp"/>

			</ScrollView>

		</LinearLayout>

	</LinearLayout>

</LinearLayout>

